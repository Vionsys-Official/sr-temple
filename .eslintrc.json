{
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "next/core-web-vitals"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/jsx-props-no-spreading": "off",
    "no-nested-ternary": "off",
    "no-use-before-define": "off",
    "@typescript-eslint/no-use-before-define": "error"
  }
}